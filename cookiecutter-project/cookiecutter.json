{
  "project_slug": "",
  "project_type": "default",
  "extra_dockerfile": "n",
  "masterdata": {
    "configs": {
      "default": {
        "image": "docker.pkg.github.com/nolte/vscode-devcontainers/commons-golang:latest",
        "vscode": {
          "workspace": "/go/src/github.com/nolte/{{cookiecutter.project_slug}}",
          "extentions": [
            "alefragnani.Bookmarks",
            "bierner.emojisense",
            "bierner.github-markdown-preview",
            "christian-kohler.path-intellisense",
            "cschleiden.vscode-github-actions",
            "davidanson.vscode-markdownlint",
            "docsmsft.docs-article-templates",
            "docsmsft.docs-markdown",
            "eamodio.gitlens",
            "EditorConfig.EditorConfig",
            "Gruntfuggly.todo-tree",
            "hbenl.vscode-test-explorer",
            "IBM.output-colorizer",
            "jrebocho.vscode-random",
            "mhutchie.git-graph",
            "mohsen1.prettify-json",
            "ms-azuretools.vscode-docker",
            "pascalreitermann93.vscode-yaml-sort",
            "quicktype.quicktype",
            "redhat.vscode-yaml",
            "shardulm94.trailing-spaces",
            "spikespaz.vscode-smoothtype",
            "stkb.rewrap",
            "tyriar.sort-lines",
            "vscode-ext.sync-rsync",
            "vscode-icons-team.vscode-icons"
          ]
        }
      },
      "iot": {
        "image": "nolte/vscode-dev-iot:latest",
        "vscode": {
          "workspace": "/workspace",
          "extentions": [
            "platformio.platformio-ide"
          ]
        }
      },
      "golang": {
        "image": "nolte/vscode-dev-golang:latest",
        "vscode": {
          "extentions": [
            "ms-vscode.go",
            "ethan-reesor.vscode-go-test-adapter"
          ]
        }
      },
      "python": {
        "image": "nolte/vscode-commons:latest",
        "vscode": {
          "extentions": [
            "LittleFoxTeam.vscode-python-test-adapter",
            "ms-python.python"
          ]
        }
      },
      "devops": {
        "image": "docker.pkg.github.com/nolte/vscode-devcontainers/devops:latest",
        "vscode": {
          "extentions": [
            "HashiCorp.terraform",
            "ms-kubernetes-tools.kind-vscode",
            "ms-kubernetes-tools.vscode-kubernetes-tools",
            "owenfarrell.vscode-vault",
            "samuelcolvin.jinjahtml",
            "ms-azuretools.vscode-docker",
            "vscoss.vscode-ansible"
          ]
        }
      }
    }
  },
  "_extensions": [
    "jinja2_ansible_filters.AnsibleCoreFiltersExtension"
  ]
}